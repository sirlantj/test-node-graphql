{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "nodemon src/index.ts",
    "build": "npx tsc",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "babel-node --extensions \".ts,.js\" src/index.ts",
    "start:prod": "node build/index.js",
    "build2": "babel src -d build --ignore=\"./src/**/*.spec.ts\" --extensions \".ts,.js\"",
    "babel-version": "babel --version",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"npm run lint && npm test && npx lint-staged && npm run sonar-scanner\"",
    "sonar-scanner": "sonar-scanner.bat"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@babel/register": "^7.17.7",
    "apollo-server-express": "^3.8.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "microdiff": "^1.3.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
